<div class="row">
  <div class="text-center">
    <h1>Artwork Categories</h1>

    <div class="categories-index">
      <ol>
        <% @categories.each do |category| %>
          <li><%= link_to category.name, category_path(category) %></li>
        <% end %>
      </ol>
    </div>
    
  </div>
</div>
